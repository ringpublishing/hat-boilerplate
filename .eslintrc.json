{
  "extends": ["next/core-web-vitals", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error", { "singleQuote": false , "endOfLine": "auto"}],
    "arrow-body-style": "off",
    "prefer-arrow-callback": "off",
    "no-html-link-for-pages": "on"
  }
}
